config_version=5

[application]

config/name="No Sound"
config/version="1.0"
run/main_scene="res://Scene/BootSplash.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/image="res://Assets/Icons/BootingSplash/1.png"
boot_splash/use_filter=false
config/icon="res://Assets/Icons/icon_resized.png"
config/windows_native_icon="res://Assets/Icons/icon_resized.ico"
boot_splash/minimum_display_time=3000

[audio]

buses/default_bus_layout="res://Audio/default_bus_layout.tres"
driver/enable_input=true

[autoload]

SettingConfig="*res://Script/Setting/SettingConfig.gd"
Global="*res://Script/Global.gd"
GlobalWorldEnvironment="*res://Scene/world_environment.tscn"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
SimpleGrass="*res://addons/simplegrasstextured/singleton.tscn"

[dialogue_manager]

general/balloon_path="res://Assets/dialogue/balloon.tscn"
general/states=["Global"]

[display]

window/size/mode=3
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/vsync/vsync_mode=0

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg", "res://addons/simplegrasstextured/plugin.cfg", "res://addons/terrain_3d/plugin.cfg")

